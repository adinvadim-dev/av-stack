{
  "name": "av-stack",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "typecheck": "turbo typecheck",
    "lint": "turbo lint",
    "test": "turbo test",
    "check": "turbo run lint typecheck test --cache-dir=.turbo",
    "check:affected": "turbo run lint typecheck test --affected --cache-dir=.turbo",
    "db:generate": "turbo db:generate",
    "db:migrate": "turbo db:migrate",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down",
    "init:template": "node scripts/init-template.mjs",
    "clean": "turbo clean"
  },
  "packageManager": "pnpm@10.28.2",
  "devDependencies": {
    "turbo": "^2.5.4"
  }
}
